{
  "name": "forbid.only",
  "version": "1.0.0",
  "description": "Forbid pull requests that still have .only tests in them",
  "author": "thislooksfun <thislooksfun@repbot.org> (https://github.com/thislooksfun/forbid.only)",
  "license": "ISC",
  "repository": "https://github.com/forbid.only.git",
  "homepage": "https://github.com/forbid.only",
  "bugs": "https://github.com/forbid.only/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "prettier --write **/*",
    "test": "prettier --check **/* && jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "acorn": "^7.1.0",
    "acorn-walk": "^7.0.0",
    "body-parser": "^1.19.0",
    "new-github-issue-url": "^0.2.1",
    "node-cmd": "^3.0.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "nock": "^11.7.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "smee-client": "^1.0.2"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
